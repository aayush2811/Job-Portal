<!DOCTYPE html>
<html lang="en">
<head>
	<title>Company Register</title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<!-- Stylesheet -->
	<link rel="stylesheet" type="text/css" href="css/stylesheet.css">
	<!-- Bootstrap 5.3 link -->
	<link href="/js/bootstrap5.3/css/bootstrap.min.css" rel="stylesheet">	
	<!-- Font-family -->	
	<link href="/font/raleway/style.css" rel="stylesheet">	
	<link href="/font/rubik/style.css" rel="stylesheet">
	<!-- Font-awesome link -->
	<link rel="stylesheet" type="text/css" href="/fonts/fontawesome/css/all.min.css">
	<link rel="stylesheet" href="css/cursor.css">
</head>
<body>
<!-- header start here -->
<div id="header"></div>
<!-- header end here -->

<!-- cursor effect  -->
<div class="cursor"></div>
<div class="circle"></div>
<!-- cursor effect end  -->

<section class="profile_banner">
	<div class="container-fluid">
		<div class="row">
			<div class="col-sm-6 nearJob">
				<h1>Your dream Job</h1>
				<h2 class="text-red">Is Near to You</h2>
			</div>
		</div>
	</div>
</section>
<!-- /. profile_banner -->

<section class="profileMain com-register">
	<div class="container-fluid">
		<div class="row">
			<div class="col-sm-3 pe-lg-5">
				<div class="card mb-2 border-0">
					<div class="jobImage">
						<div class="image-card">
							<img class="card-img-top" src="images/company-register.png" alt="Profile">
						</div>
					</div>
				</div>
				
				<div class="proLinks">
					<div class="d-grid">
					<a href="javascript:void(0);" class="btn btn-outline-danger mb-4">
						<i class="fa fa-cloud me-2" aria-hidden="true"></i> General Information
					</a> 
					</div>
					<div class="d-grid">
					<a href="javascript:void(0);" class="btn btn-outline-danger mb-4">
						<i class="fa fa-user me-2" aria-hidden="true"></i> Company Address
					</a>
					</div>
					<div class="d-grid">
					<a href="javascript:void(0);" class="btn btn-outline-danger mb-4">
						<i class="fa fa-key me-2" aria-hidden="true"></i> Social Account
					</a>
					</div>
				</div>
			</div><!-- /. grid column -->
			<div class="col-sm-9 profile_right">
				<div class="border p-4 h-100">
					<form method="post">	
						<div class="pb-5">
							<h3>General Information
								<span class="float-end fx-top">
									<a href="javascript:void(0);" class="btn btn-danger rounded-1">
										<i class="fa fa-upload me-2" aria-hidden="true"></i> Upload Your Documents
									</a>
								</span>
							</h3>
						</div>
						<div class="form-group row">	
							<div class="col-sm-6">
								<input type="text" name="name" id="name" class="form-control" placeholder="Name" value="" required>
							</div>
							<div class="col-sm-6">
								<input type="text" name="tagline" id="tagLine" class="form-control" placeholder="Company Tagline" value="" required>
							</div>
						</div>
						<div class="form-group row">	
							<div class="col-sm-6">
								<select class="form-control" name="category" id="category">
									<option>Category</option>
								</select>
							</div>
							<div class="col-sm-6">
								<input type="text" name="ownername" id="qwnerNmae" class="form-control" placeholder="Owner Name" value="" required>
							</div>
						</div>
						<div class="form-group row">	
							<div class="col-sm-6 dropdown-container position-relative">
								<select name="location" id="location" class="form-control">
									<option value="location">Loaction</option>
									<option value="newyork">New York</option>
									<option value="losangeles">Los Angeles</option>
									<option value="shicago">Shicago</option>
									<option value="uniteskingdom">United Kingdom</option>
								</select>
							</div>
							<div class="col-sm-6">
								<div class="fileUpload">
									<label class="upload form-control">
										Company Logo
										<input name="Image" id="uploadFile" type="file">
										<span class="chooseFile">Choose File</span>
									</label>
								</div>
							</div>
						</div>
						<div class="form-group row">	
							<div class="col-sm-6 dropdown-container position-relative">
								<select name="established" id="established" class="form-control">
									<option value="age">Established</option>
								</select>
							</div>	
							<div class="col-sm-6">
								<input type="number" name="mobile" id="mobileNo" class="form-control" placeholder="Mobile Number" value="" required>
							</div>
						</div>
						<div class="form-group row">	
							<div class="col-sm-6">
								<input type="text" name="landline" id="land-line" class="form-control" placeholder="Land Line" value="" required>
							</div>
							<div class="col-sm-6">
								<input type="email" name="email" id="e_mail" class="form-control" placeholder="E-mail Address" value="" required>
							</div>
						</div>
						<div class="form-group mt-5">
							<div class="d-grid">
							<input type="submit" name="save" class="btn btn-danger rounded-0" id="save" value="Save & Next">
							</div>
						</div>
					</form>
					<!-- /. Protfolio -->
				</div>
			</div>
		</div>
	</div>
</section>
<!-- /. profileMain -->

<!-- footer area start -->
<div id="footer"></div>
<!-- footer area end -->

<!-- Js Links -->
<script src="/js/bootstrap5.3/js/popper.min.js"></script>
<script src="/js/jquery-3.6.0.min.js"></script>
<script src="/js/bootstrap5.3/js/bootstrap.bundle.min.js"></script>
<script src="/js/common.js"></script>
<script src="js/cursor.js"></script>
</body>
</html>